{
  "expo": {
    "scheme": "nittany-navigator",
    "name": "NittanyNavigator",
    "slug": "nittany-navigator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to capture event photos.",
        "NSPhotoLibraryUsageDescription": "This app accesses the photo library to select event photos."
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.bardanphuyel.nittany-navigator"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bardanphuyel"
    },
    "notification": {
      "icon": "./assets/favicon.png",
      "color": "#fff"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e3d5daae-29c8-4f97-b0f9-2782b2642383"
      }
    },
    "owner": "bardanp"
  }
}
